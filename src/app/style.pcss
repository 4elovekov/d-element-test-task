/* stylelint-disable custom-property-pattern */

* {
	padding: 0;
	margin: 0;
	border: none;
	text-decoration: none;
}

:root {
	--color-blue1: rgba(31, 63, 104, 1);
	--color-blue2: rgba(49, 96, 153, 1);
	--color-blue3: rgba(53, 110, 173, 1);
	--color-blue4: rgba(90, 152, 208, 1);
}

@font-face {
    font-family: "Gilroy rg";
    src: url('/fonts/Gilroy-Regular.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}